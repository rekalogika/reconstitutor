{
    "name": "rekalogika/reconstitutor",
    "description": "A small layer above Doctrine events to help you reconstitute/hydrate your entities. The most common example being handling file uploads, but also many other purposes. It lets you augment Doctrine's hydration with your logic in a concise and expressive class.",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Rekalogika\\Reconstitutor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rekalogika\\Reconstitutor\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Priyadi Iman Nurcahyo",
            "email": "priyadi@rekalogika.com"
        }
    ],
    "require": {
        "doctrine/orm": "^2.16",
        "rekalogika/direct-property-access": "^1.1.2 || ^1.2",
        "symfony/config": "^6.0",
        "symfony/dependency-injection": "^6.0",
        "symfony/http-kernel": "^6.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.6",
        "vimeo/psalm": "^5.15",
        "psalm/plugin-phpunit": "^0.18.4",
        "psalm/plugin-symfony": "^5.0",
        "symfony/phpunit-bridge": "^6.0"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.1-dev"
        }
    }
}
